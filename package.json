{
  "name": "vite-plugin-imock",
  "version": "0.0.2",
  "description": "Mock Data using Vite DevServer",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "npm run build -- --watch --ignore-watch examples",
    "build": "tsup src/index.ts --dts --format cjs,esm",
