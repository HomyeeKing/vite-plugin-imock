    "plugin",
    "mock",
    "mockjs"
  ],
  "author": "HomyeeKing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HomyeeKing/vite-plugin-imock/issues"
  },
  "homepage": "https://github.com/HomyeeKing/vite-plugin-imock#readme",
  "peerDependencies": {
    "mockjs": ">=1.1.0",
    "vite": ">=2.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.6.4",
    "@types/node": "^15.0.2",
    "eslint": "^7.25.0",
    "mockjs": "^1.1.0",
    "tsup": "^4.10.1",
    "typescript": "^4.2.4",
    "vite": "^2.2.4"
  }
